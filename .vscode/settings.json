{
    "conventionalCommits.scopes": ["chess-legal-moves", "front", "back", "e2e"],
    "go.toolsEnvVars": {
        "GOOS": "js",
        "GOARCH": "wasm"
    },
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.configPath": "./front/.prettierrc.json",
    "[go]": {
        "editor.defaultFormatter": "golang.go",
        "editor.formatOnSave": true
    }
}
